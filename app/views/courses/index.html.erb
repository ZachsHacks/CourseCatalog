<%=pretty_print_courses(@courses)%>

<script>
$("button[id=user]").click(function(){
	if (document.getElementById("user").name=="enroll") {
		value=document.getElementById("user").value;
		$("#div1").load("#{enrollments_create_path}"+value);
	}else{
		value=document.getElementById("user").value;
		$("#div1").load("#{enrollments_destroy_path}"+value);
	}
});
</script>
